  "scripts": {
    "build": "ember build",
    "build:prod": "ember build --prod -o dist-prod",
    "build:prod:local": "IS_LOCAL_PROD=1 yarn run build:prod",
    "build:prod:local:watch": "yarn run build:prod:local --watch",
    "start": "IS_ES6_DEV=1 ember serve",
    "start:api:local": "DEV_API_HOST='http://localhost:16006' yarn start",
    "start:debug": "LOUD_DEBUG=1 ENABLE_DEPRECATION_WARNINGS=1 yarn run start",
    "start:prod:local": "IS_LOCAL_PROD=1 ember s --prod -o dist-prod -p 4201 -lr false --host clique.local",
    "start:prod:api:local": "PROD_API_HOST='http://localhost:16006' start:prod:local",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "test": "yarn run lint:scss && ember exam:iterate 4",
    "lint:js:fix": "yarn run lint:js --fix",
    "lint:scss": "stylelint \"app/styles/**/*.scss\"",
    "lint:scss:fix": "yarn run lint:scss -- --fix",
    "test:auto": "ember exam --test-port 7358 --server --launch Chrome --random",
    "test:api:local:auto": "TEST_API_HOST='http://localhost:16007' ember exam --test-port 7358 --server --launch Chrome --random",
    "test:auto:coverage": "COVERAGE=true yarn run test:auto",
    "test:api:local:auto:coverage": "COVERAGE=true yarn run test:api:local:auto",
    "start:coverage": "mkdir -p ./coverage && ./node_modules/.bin/live-server --port=5200 ./coverage/",
    "deploy:prod": "ember deploy production --activate --verbose",
    "devDeploy": "echo 'DEPRECATION WARNING: use yarn run deploy:prod' && yarn run deploy:prod"
  },
